{
  "manifest_version": 2,
  "name": "Deep Dive",
  "version": "0.1",
  "description": "Deep link to any text on the web.",

  "icons": {
    "48": "icons/deep-dive-48.png",
    "96": "icons/deep-dive-96.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dive.js"]
    }
  ]
}
